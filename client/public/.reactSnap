{
  "viewport": {
    "width": 1200,
    "height": 800
  },
  "include": [
    "/",
    "/dieu-khoan-su-dung",
    "/chinh-sach-bao-mat",
    "/tim-kiem",
    "/auth"
  ],
  "fixWebpackChunksIssue": false,
  "skipThirdPartyRequests": true,
  "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
  "puppeteerOptions": {
    "cache": true
  },
  "destination": "./dist",
  "minifyHtml": {
    "collapseWhitespace": true,
    "removeComments": true
  },
  "crawl": true,
  "userAgent": "ReactSnap",
  "https": false,
  "port": 5000,
  "inlineCss": false,
  "preconnectThirdParty": false,
  "removeBlobs": true,
  "cacheAjaxRequests": false
}